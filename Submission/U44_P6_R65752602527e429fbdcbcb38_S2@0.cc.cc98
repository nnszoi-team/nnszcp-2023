#include<bits/stdc++.h>
using namespace std;
int main(){
	int n,a[200002],ans=0;
	cin>>n;
	for(int i=0;i<n;i++){
		cin>>a[i];
	}for (int i = 0; i <n; i++) {
        for (int j = 0; j < n; j++) {
            if (a[i] < a[j]) {
                
                ans++;
            }
        }
    }
	cout<<"YES"<<endl<<ans;
	return 0;
}