#include<bits/stdc++.h>
using namespace std;
int main(){
	string str;
	cin>>str;
	if(str.size() <= 3){
		cout<<"NO";
		return 0;
	}
	for(int i=0;i<str.size()-3;i++){
		if(str[i]=='n'&&str[i+1]=='n'&&str[i+2]=='s'&&str[i+3]=='z'){
			cout<<"YES";
			return 0;
		}
	}
	cout<<"NO";
	return 0;
}