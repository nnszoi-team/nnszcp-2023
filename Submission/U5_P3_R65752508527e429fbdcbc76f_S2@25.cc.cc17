#include<bits/stdc++.h>
#include<string.h>
using namespace std;
int main()
{
   int n, k=0,p=0;
   cin>>n;
   int a[n]={}, b[n] = {};
   for(int i=1;i<=n;i++)
   {
   	cin>>a[i];
   	if(a[i] > k){k=a[i];}
   	if(i>=1)
   	{
   		b[i] = a[i] - a[i-1];
   		if(b[i]>p){p = b[i];}

	   }
   }\
   cout<<k<<endl<<p;
   return 0;
}