#include<bits/stdc++.h>
using namespace std;
string str;
int main(){
	cin>>str;
	for(int i=0;i<str.size();i++){
		if(str[i]=='n'){
			if(str[i+1]=='n'){
				if(str[i+2]=='s'){
					if(str[i+3]=='z'){
						printf("YES");
						return 0;
					} 
				}
			}
		}
	}
	printf("NO");
	return 0;
}