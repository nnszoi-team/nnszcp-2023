#include<bits/stdc++.h>
using namespace std;
const int N=2005,M=100005;
long long h[N],s[M];
int main(){
	int n,m;
	scanf("%d",&n);//h
	scanf("%d",&m);//s
	for(int i=1;i<=n;++i){
		scanf("%lld",&h[i]);
	}
	for(int i=1;i<=m;++i){
		scanf("%lld",&s[i]);
	}
	int ans=((n-1)*n)/2;
	for(int i=1;i<=m;++i){
		printf("%d\n",ans);
	}
	return 0;
}