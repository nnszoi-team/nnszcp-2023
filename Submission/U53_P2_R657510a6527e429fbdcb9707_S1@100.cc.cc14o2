#include <bits/stdc++.h>

using namespace std;

int main()
{

    string a;
    bool loca[200];

    cin>>a;
    for(int i=0;i<a.size();i++){
        if(a[i]=='n')loca[i]=1;
    }
    for(int i=0;i<a.size();i++){
        if(loca[i]){
            if(a[i+1]=='n' && a[i+2]=='s' && a[i+3]=='z'){cout<<"YES";return 0;}

        }
    }

    cout<<"NO";



    return 0;
}